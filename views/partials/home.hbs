<div class="container-fluid">
  <div class="row full">
    <div class="col-md-3 full bg-brand-primary">
      <img class="circular" src="/assets/img/sign.jpg" style="position: absolute; top: 100px; right: -150px; width: 300px;">
    </div>
    <div class="col-xs-12 col-md-9 full">
      <div class="row">
        <div class="col-xs-12 full">
          <div class="text-center">
            <h1 class="h2">Dr. Robert J Gilbert D.C., DABCI</h1>
          </div>
          <div style="overflow-y: scroll; max-height: 70%; margin-left: 165px">
            {{> toggle }}
          </div>
        </div>
      </div>
      <div class="row" style="height: auto">
        <div class="col-xs-12" style="position: relative">
          <div class="bg-white" style="bottom: 0; width: 98%; min-height: 90px; position: absolute">
            <div hover-text="menu" class="text-center mtj" style="width: 100%">
              <p ng-show="_hover.menu" ng-bind="_hover.menu"></p>
              {{> jump-nav }}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
